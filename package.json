{
  "name": "grin-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:dev": "pnpm generate:localEnv && cross-env NODE_ENV=development next dev",
    "build:prod": "pnpm generate:localEnv && rimraf dist && cross-env NODE_ENV=production next build && pnpm copy:static",
    "build:test": "pnpm generate:localEnv && rimraf dist && cross-env NODE_ENV=test next build && pnpm copy:static",
    "copy:static": "ts-node --project ./tsconfig.node.json ./scripts/copy-static.ts",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "prepare": "husky install",
    "commit": "git add . && git cz",
    "preinstall": "npx only-allow pnpm",
    "serve": "cd ./dist && node server.js",
    "generate:localEnv": "ts-node --project ./tsconfig.node.json ./scripts/resolve-ip.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.0",
    "@ant-design/pro-components": "^2.7.14",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@handsontable/react": "^14.4.0",
    "@iconify-icon/react": "^2.1.0",
    "@iconify/react": "^5.0.2",
    "ahooks": "^3.8.1",
    "antd": "^5.19.2",
    "aws4": "^1.13.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.12",
    "framer-motion": "^11.2.13",
    "handsontable": "^14.4.0",
    "ip": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.1",
    "next": "14.2.15",
    "next-intl": "^3.21.1",
    "nodemailer": "^6.9.15",
    "react": "^18.3.1",
    "react-activation": "^0.12.4",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "swiper": "^11.1.14",
    "swr": "^2.2.5",
    "uuid": "^10.0.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/ip": "^1.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog-zh": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "husky": "8.0.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.75.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  }
}
